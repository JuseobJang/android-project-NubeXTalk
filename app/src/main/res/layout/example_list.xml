<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:padding="@dimen/padding_l"
    tools:context=".Example.ExampleList">

    <Button
        android:id="@+id/btnAnim"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="AnimManager(애니메이션 라이브러리)"/>

    <Button
        android:id="@+id/btnFont"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Iconify(아이콘 라이브러리)"/>

    <Button
        android:id="@+id/btnHttp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HttpManager(Http 통신 라이브러리)"/>

    <Button
        android:id="@+id/btnJsoup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="JSOUP(HTML Parsing 라이브러리)"/>

</LinearLayout>